<h1>Become a Teacher here!</h1>
<div class="container">
  <div class="row">
    <div class="col-xs-12">

      <%= simple_form_for @establishment do |f| %>
      <% f.error_notification %>
      <%= f.input :name %>
      <%= f.input :city %>
      <%= f.input :country %>
      <%= f.input :street %>
      <%= f.input :postal_code %>
      <%= f.input :description %>
      <%= f.button :submit %>
      <% end %>
    </div>
  </div>
</div>
